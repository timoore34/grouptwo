<UserControl x:Class="GM_Toolbox.ControlElements.CheckBoxDropdownBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GM_Toolbox.ControlElements">
    <StackPanel Orientation="Horizontal">
        <Viewbox x:Name="CheckSize"
                 Margin="5 5 10 5">
            <CheckBox x:Name="EnableCheckBox"
                      VerticalContentAlignment="Center"/>
        </Viewbox>
        <ComboBox x:Name="ListDropDown"
                  Width="50"
                  Margin="5"/>
    </StackPanel>
</UserControl>
